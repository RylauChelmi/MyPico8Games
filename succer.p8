end


function jersey_color(f)
	pal(8, f.teamcolors[1])
	pal(2, f.teamcolors[2])
end


function draw_footballer(f)
 local animspr = f.lastspr
 local	animflip = f.lastflip
 local animfactor = 1
 local animoffset = 20
 local anim_end = 1
 local dirx =	f.dirx
	local diry = f.diry

 jersey_color(f)

 if (f.vel>0.1) then
 	animfactor = 4
 	animoffset = 0
 	animflip = true
  anim_end = 4
--  local cos22_5 = 0.9239*f.damp
--  local sin22_5 = 0.3827*f.damp
  local cos22_5 = 0.9239
  local sin22_5 = 0.3827
  dirx=f.dx/f.vel
  diry=f.dy/f.vel
  if (dirx<-cos22_5) then -- full left
  	animspr=2
  else
  	if (dirx<-sin22_5) then -- diag left...
  		if (diry<0) then -- ...and up
    	animspr=1
  		else -- ...and down
    	animspr=3
  		end
  	else
		 	animflip = false
 			if (dirx<sin22_5) then -- full...
	  		if (diry<0) then -- ...up
			  	animspr=0
	  		else -- ...down
			  	animspr=4
	  		end
	  	else
    	if (dirx<cos22_5) then -- diag right...
    		if (diry<0) then -- ...and up
      	animspr=1
  		  else -- ...and down
      	animspr=3
  		  end
  		 else -- full right
  		 	animspr=2
  		 end
  		end
  	end
  end
 end

 f.animtimer += f.vel * 0.25
 while (flr(f.animtimer)>=anim_end) do
  f.animtimer -= anim_end
 end

 f.lastspr = animspr
 f.lastflip = animflip
 f.dirx =	dirx
	f.diry = diry

 local pos = footer_sprite_pos(f)
 spr(animoffset+animspr*animfactor+f.animtimer,pos.x,pos.y,1,1,animflip)
-- spr(animoffset+animspr*animfactor+f.animtimer,f.x-f.w,f.y-f.h,1,1,animflip)
end


function footer_sprite_pos(f)
	return { x = f.x-f.w, y = f.y-f.h }
end


function create_footballer(i)
	f = {}
	f.x = 64
	f.y = 64
	f.w = 4
	f.h = 8
	f.dx = 0
	f.dy = 0
	f.vel = 0
	f.dirx = 0
	f.diry = 1
	f.lastvel = 0
	f.lastspr = 4
	f.animtimer = 0
	f.damp = 0.9
	f.lastflip = false
	f.justshot = false
	if (i%2==0) then
		f.teamcolors = {8,2}
	else
		f.teamcolors = {12,13}
	end

	f.draw = function(fo)
	 fo.state.draw(fo) 
 end
 
 f.update = function(fo)
	 fo.state.update(fo) 
 end

	f.drawshadow = draw_footballershadow
	f.state = state_ok
	
	f.set_state=function(fo,st)
	 fo.state = st
	 if (st.start != nil) then
   st.start(fo)
  end
 end

	return f
end

