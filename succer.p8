sqrt0 = sqrt
function sqrt(n)
  if (n <= 0) return 0
  if (n >= 32761) return 181.0
  return sqrt0(n)
end


ball={}
ball.x=64
ball.y=32
ball.w=2
ball.h=4
ball.dx=0
ball.dy=0
ball.damp=0.99
ball.draw = draw_ball
ball.drawshadow = draw_ballshadow



function touch_ball(m,dist)
 return (m.x-dist<ball.x and ball.x<m.x+dist and m.y-dist<ball.y and ball.y<m.y+dist)
end


function dribble_ball(m)
	if (justshot) then
		justshot=false
		return
	end
	local dribbledist=4
 if touch_ball(m, dribbledist) then
 	local dribble = 1.1
 	ball.dx = m.dx*dribble
 	ball.dy = m.dy*dribble
 	spawn_smokes_kick(ball.x, ball.y, rnd(m.vel))
 end
end


function update_ball()
 ball.x += ball.dx
 ball.y += ball.dy
 ball.dx *= ball.damp
 ball.dy *= ball.damp
 if (ball.x<0) ball.dx=abs(ball.dx) ball.x=0
 if (ball.x>127) ball.dx=-abs(ball.dx) ball.x=127
 if (ball.y<0) ball.dy=abs(ball.dy) ball.y=0
 if (ball.y>127) ball.dy=-abs(ball.dy) ball.y=127
end


function bubble_sort(t)
	local len = #t
	local active = true
	local tmp = nil
	while active do
		active = false
		for i=1,len-1 do
			if (t[i+1].y < t[i].y) then
				tmp = t[i]
				t[i] = t[i+1]
				t[i+1] = tmp
				active = true
			end
		end
	end
end


players = {}


function create_player(i)
	local player = {}
	player.man = nil
	player.num = i
	player.but = 0
	add(players,player)
end


function distance(a, b)
	local d={x = a.x-b.x, y = a.y-b.y}
 return sqrt(d.x*d.x + d.y*d.y)
end


function kick(p)
 -- direction
 local x = 0
 local y = 0
	if (btn(0,p.num)) x -= 1
	if (btn(1,p.num)) x += 1
	if (btn(2,p.num)) y -= 1
	if (btn(3,p.num)) y += 1

 -- tmp
 local kickfactor= 1.0+0.2*p.but
 x = p.man.dirx*kickfactor
 y = p.man.diry*kickfactor
 ball.dx+=x
 ball.dy+=y
 justshot = true
 spawn_smokes_kick(ball.x, ball.y, kickfactor)
end


function tackle(p)
	p.man.set_state(p.man, create_state_tackle())
end


function button_released(p)
	local kickdist=7
	if (touch_ball(p.man, kickdist)) then
		kick(p)
 else
 	tackle(p)
	end
	p.but = 0
end


function player_input(p)
	if (p.man.state.input != nil) then
  p.man.state.input(p)
 else
 end
end


function damp(man)
 man.dx *= man.damp
 man.dy *= man.damp
end


function ok_input(p)
 if (p.man==nil) return
 local man = p.man
 local vel_inc = 0.2

 damp(man)

	if (btn(0,p.num)) man.dx -= vel_inc
	if (btn(1,p.num)) man.dx += vel_inc
	if (btn(2,p.num)) man.dy -= vel_inc
	if (btn(3,p.num)) man.dy += vel_inc

	if (btn(4,p.num)) then
		p.but = min(p.but+1, 20)
 else if p.but > 0 then
   button_released(p)
   return
  end
 end

 noinput=true

 vel = sqrt(man.dx*man.dx + man.dy*man.dy)
 man.vel = vel
 local max_vel = 1.5
 if (vel > max_vel) then
  man.dx *= max_vel/vel
  man.dy *= max_vel/vel
  man.vel = max_vel
 end
 local min_vel = 0.1
	if (vel>0.1) then
		man.lastdx = man.dx
		man.lastdy = man.dy
 end
end


function footer_physics(man)
 local newposx = man.x+man.dx
 local newposy = man.y+man.dy
 if (newposx < 0 or newposx > field.w) then
 	newposx = man.x 
 	man.dx = 0
 end
 if (newposy < 0 or newposy > field.h) then
 	newposy = man.y
 	man.dy = 0
 end
 man.x = newposx
 man.y = newposy
end


function man_update(f)
	if (f.state.update != nil) then
  f.state.update(f)
 end
 footer_physics(f)
end


